<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="600.0" prefWidth="800.0"
			style="-fx-font-family: 'Courier New'; -fx-font-size: 16;"
			xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
			fx:controller="cn.jeff.app.d2de.MainFragmentJ">
	<top>
		<FlowPane alignment="CENTER_LEFT" hgap="10.0" vgap="10.0" BorderPane.alignment="CENTER">
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
			</padding>
			<Button fx:id="btnOpen" mnemonicParsing="false" onAction="#btnClick" text="打开"/>
			<Button fx:id="btnSave" disable="true" mnemonicParsing="false" text="保存修改"/>
		</FlowPane>
	</top>
	<left>
		<VBox prefWidth="350.0" spacing="4.0" BorderPane.alignment="CENTER">
			<padding>
				<Insets bottom="4.0" left="4.0" right="4.0" top="4.0"/>
			</padding>
			<TextField fx:id="tfUnitNameFilter"/>
			<ListView fx:id="lvUnitName" VBox.vgrow="ALWAYS"/>
		</VBox>
	</left>
	<bottom>
		<HBox alignment="CENTER_LEFT" spacing="10.0" BorderPane.alignment="CENTER">
			<Button mnemonicParsing="false" text="Button"/>
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
			</padding>
		</HBox>
	</bottom>
	<center>
		<TableView fx:id="mainTableView"/>
	</center>
</BorderPane>
